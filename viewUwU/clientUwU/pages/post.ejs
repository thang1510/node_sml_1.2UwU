<% layout('/clientUwU/partial/boilerplate') -%>


<!-- Post Content -->
<article>
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-10 mx-auto">
        <h2 class="section-heading"><%= operator_name %></h2>
        <blockquote class="blockquote"><%= id %></blockquote>
        <h2 class="section-heading">Arknights is love.</h2>
        <a href="#">
          <img class="img-fluid" src="/public/img/<%= operator_name %>/1.png" alt="">
          <% for (j = 1; j < 5; j += 1) { %>
            <img class="img-fluid" src="/public/img/<%= operator_name %>/<%= j + 1 %>.jpg" alt="">
          <% } %>
          <img class="img-fluid" src="https://i.pinimg.com/originals/c1/51/f9/c151f9cac90e6bcd7d98f50f1c7252af.jpg" alt="">
        </a>
        <span class="caption text-muted">Arknights is life.</span>
        <p>Placeholder text by
          <a href="http://spaceipsum.com/">Space Ipsum</a>. Photographs by
          <a href="https://www.flickr.com/photos/nasacommons/">NASA on The Commons</a>.</p>
      </div>
    </div>
  </div>
</article>

<hr>
